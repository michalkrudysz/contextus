    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.8",
    "openai": "^4.51.0",
    "socket.io": "^4.7.5",
    "uuid": "^10.0.0",
    "ws": "^8.17.0"
